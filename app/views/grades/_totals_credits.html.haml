%table.no_border{style: 'width: 280px'}
	%tr.text-center
		%th{colspan: "2"} Créditos:
	%tr
		%td.text-end Nro. Créditos Cursados (UC):
		%td.text-end.fw-bold= grade.total_credits_coursed
	%tr
		%td.text-end Nro. Créditos Aprobados (Sin EQ):
		%td.text-end.fw-bold= grade.total_credits_approved
	%tr
		%td.text-end Nro. Créditos por EQ:
		%td.text-end.fw-bold= grade.total_credits_eq
	%tr
		%td.text-end Nro. Créditos Aprobados:
		%td.text-end.fw-bold= grade.total_credits_approved_or_eq
	%tr
		%td.text-end Eficiencia:
		%td.text-end.fw-bold= grade.efficiency
